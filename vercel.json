{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        },
        {
            "src": "*.css",
            "use": "@vercel/static"
        },
        {
            "src": "*.js",
            "use": "@vercel/static"
        },
        {
            "src": "*.png",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/style.css",
            "dest": "/style.css"
        },
        {
            "src": "/script.js",
            "dest": "/script.js"
        },
        {
            "src": "/logo.png",
            "dest": "/logo.png"
        },
        {
            "src": "/dashboard.html",
            "dest": "/dashboard.html"
        },
        {
            "src": "/login",
            "dest": "/server.js"
        },
        {
            "src": "/callback",
            "dest": "/server.js"
        },
        {
            "src": "/logout",
            "dest": "/server.js"
        },
        {
            "src": "/api/(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/",
            "dest": "/index.html"
        }
    ]
}
